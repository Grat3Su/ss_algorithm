package online;

import java.util.Scanner;

public class SquareNumberTest {
	static int X, N;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		X = sc.nextInt();
		N = sc.nextInt();
	}
	//재귀 : 분할정복 미적용
	//X^n = X*X^n-1
	//X^n-1 = X*X^n-2
	static long exp1(long x, int n) {
		return x*exp1(x, n-1);
	}

}
