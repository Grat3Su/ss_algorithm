package BOJ.offline0830;
import java.util.*;
import java.io.*;
public class BOJ_17070_MovePipe {
	static Pipe head;
	static Pipe tail;
	static int N, count;
	static int[][] map;
	static int[] dx = {1,1,0};
	static int[] dy = {0,1,1};
	public static void main(String[] args) throws IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		N = Integer.parseInt(br.readLine());
		map = new int[N][N];
		for(int i = 0; i<N; i++) {
			StringTokenizer st = new StringTokenizer(br.readLine());
			for(int j = 0; j<N; j++) {
				map[i][j] = Integer.parseInt(st.nextToken());
			}
					
		}
		head = new Pipe(1,0);
		tail = new Pipe(0,0);
		bfs();
		System.out.println(count);
	}
	
	static void bfs() {
		Queue<int[]> q = new ArrayDeque<int[]>();
		q.offer(new int[]{head.x, head.y});
		
		while(q.isEmpty()) {
			int[] pos = q.poll();			
			
			for(int i = 0; i<3; i++) {
				int x = pos[0]+dx[i];
				int y = pos[1]+dy[i];
				if(x==N-1&&y==N-1) {
					count++;
				}
				if(x>=N||y>=N) continue;
				if(map[x][y]!= 0)	continue;
				q.offer(new int[] {x,y});
			}
		}
	}
	
	static class Pipe{
		int x, y;

		public Pipe(int x, int y) {
			super();
			this.x = x;
			this.y = y;
		}
	}
}
