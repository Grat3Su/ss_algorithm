package basic.perm;

import java.util.Arrays;

public class Perm_NP {

	static int[] src = {3,1,5,4,2};//정렬되지 않은 수. 정렬하지 않으면 이거보다 더 큰 수로 수열이 만들어짐
	public static void main(String[] args) {
		//NP를 위한 정렬 <= 가장 작은 값을 구성하도록
		Arrays.sort(src);

	}
	
	static boolean np() {
	//3가지
	//앞에서 교환되어야 하는 인덱스&작업
		int i = src.length;
		//뒤에서 교환되어야 하는 인덱스&작업
		//교환 후 뒤쪽을 작은 수로 정리
	}

}
