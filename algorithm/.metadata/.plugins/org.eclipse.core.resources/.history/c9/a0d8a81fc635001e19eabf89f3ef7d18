package hw;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.StringTokenizer;

/*
 * 데이터셋
4
3 45
20 20 20
6 10
1 2 5 8 9 11
4 100
80 80 60 60
4 20
10 5 10 16	// 테스트 케이스 개수
// 첫 번째 테스트 케이스 N = 3, M = 45
// 3개의 과자 봉지 무게 a1 = 20, a2 = 20, a3 = 20
 */
public class hwalgo06_부울경_3반_장수영 {
static int[] tgt = new int[2];
	public static void main(String[] args) throws IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));

		int T = Integer.parseInt(br.readLine());
		StringTokenizer st = new StringTokenizer(br.readLine());
		int N = Integer.parseInt(st.nextToken());//배열 크기
		int M = Integer.parseInt(st.nextToken());//무게
		int[]A = new int[N];
		
		st = new StringTokenizer(br.readLine());
		for(int i = 0; i<N; i++) {
			A[i] = Integer.parseInt(st.nextToken());
		}
		snack(N,0);
		
	}
	
	static void snack(int N, int tgtidx) {
		if(tgtidx > N) {
			return;
		}
		
		
	}

}
