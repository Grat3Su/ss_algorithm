package basic.perm;

import java.util.Scanner;

public class NMPerm {
	static int[]arr;
	static int[]tgt;
	static boolean[] check;
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int M = sc.nextInt();
		
		arr = new int[N];
		tgt = new int[M];
		check = new boolean[N];
		
		for(int i = 0; i<N; i++)
			arr[i] = i+1;
	}
	
	static void perm(int tgtidx) {
		if(tgtidx == tgt.length)
		{
			for(int i = 0; i<tgt.length; i++)
			{
				System.out.printf("%d ", tgt[i]);
			}
			return;
		}
		
		for(int i = 0; i<arr.length; i++) {
			if(check[i] == true)	continue;
			tgt[tgtidx] = arr[i];
			check[i] = true;
			perm(tgtidx+1);
			check[i] = false;
		}
			
	}

}
